{
  "name": "Pinterest-Clone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "dev": "concurrently --kill-others-on-fail \"nodemon start\" \"nodemon --exec npm run client \"",
    "lint": "./node_modules/.bin/eslint server client tests",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@mui/styles": "^5.3.0",
    "aws-sdk": "^2.1073.0",
    "axios": "latest",
    "body-parser": "~1.18.2",
    "cookie-session": "^1.3.3",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.12",
    "morgan": "~1.9.0",
    "passport": "^0.5.3",
    "passport-google-oauth": "^2.0.0",
    "passport-twitter": "latest",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-masonry-component": "^6.3.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^5.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/eslint-parser": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.4.2",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^6.5.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "nock": "^13.2.2",
    "nodemon": "^2.0.15",
    "postcss-loader": "^3.0.0",
    "redux-logger": "^3.0.6",
    "sass": "^1.45.2",
    "sass-loader": "^10",
    "style-loader": "^0.23.1",
    "supertest": "^6.2.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "jest": {
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "babel-jest",
      "\\.(css|less|scss)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./testingSetup/enzymeSetup.js",
      "./testingSetup/jestSetup.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/myscripts/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14.18.0",
    "npm": "6.14.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dereje1/Pinterest-Clone.git"
  },
  "author": "Dereje Getahun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dereje1/Pinterest-Clone/issues"
  },
  "homepage": "https://dereje-pclone.herokuapp.com/"
}
